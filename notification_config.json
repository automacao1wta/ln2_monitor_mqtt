{
  "mode": "listener",
  "polling_interval_minutes": 10,
  "connection_timeout_hours": 1.0,
  "connection_check_enabled": true,
  "max_notifications_per_device_per_hour": 20,
  "recovery_notifications_enabled": true,
  "group_notifications_same_device": true,
