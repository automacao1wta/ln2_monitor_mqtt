    "ln2_battery_status": "04",
    "ln2_foam_status": "04"
  },
  "field_severities": {
    "ln2_level_status": "high",
    "ln2_angle_status": "low",
    "ln2_battery_status": "high",
    "ln2_foam_status": "low",
    "connection": "high"
  }
}
